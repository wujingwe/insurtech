{
  "manifest_version": 3,
  "name": "保險存摺保單顯示器",
  "version": "1.0.2",
  "description": "Reveal insurance details on insurtech.lia-roc.org.tw",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://insurtech.lia-roc.org.tw/*"],
      "js": ["messaging.js", "content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://insurtech.lia-roc.org.tw/*"],
      "js": ["inject.js"],
      "world": "MAIN"
    }
  ]
}
